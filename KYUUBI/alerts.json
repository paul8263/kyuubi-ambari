{
  "KYUUBI": {
    "service": [],
    "KYUUBI_SERVER": [
      {
        "name": "KYUUBI_SERVER",
        "label": "Kyuubi server",
        "description": "This host-level alert is triggered if the server port is unreachable.",
        "interval": 1,
        "scope": "ANY",
        "source": {
          "type": "PORT",
          "uri": "{{kyuubi-defaults/kyuubi_frontend_bind_port}}",
          "default_port": 10009,
          "reporting": {
            "ok": {
              "text": "TCP OK - {0:.3f}s response on port {1}"
            },
            "warning": {
              "text": "TCP OK - {0:.3f}s response on port {1}",
              "value": 1.5
            },
            "critical": {
              "text": "Connection failed: {0} to {1}:{2}",
              "value": 5
            }
          }
        }
      }
    ]
  }
}